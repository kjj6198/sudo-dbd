


<div class="row">
  <div class="dbd_nav">
    <div class="dbd_logo"><%= image_tag 'logo.png' %></div>
  </div>
</div>
<div class="row center">
  <a class="dbd_button" href="<%= new_restaurant_path %>">新增餐廳</a>
</div>
<div class="row">
<% @restaurants.each do |rest| %>
<div class="col s12 m6 l4">
  <div class="restaurant_card center">
  	<h6 class="card_title"><%= rest.name %></h6>
    <h6 class="phone"><%= rest.phone %></h6>
    <span class="dbd_icon-star dbd_icon--selected"></span>
    <span class="dbd_icon-star dbd_icon--selected"></span>
    <span class="dbd_icon-star dbd_icon--selected"></span>
    <span class="dbd_icon-star dbd_icon--selected"></span>
    <span class="dbd_icon-star dbd_icon--selected"></span>
    <div class="card_action">
      <span class="dbd_icon-pencil"></span>
      <span class="dbd_icon--selected dbd_icon-remove"></span>
    </div>
  </div>
  
</div>
<% end %>
</div>
<%= content_for :javascripts do %>
<%= javascript_include_tag 'bundle/restaurants_index_bundle'%>
<% end %>